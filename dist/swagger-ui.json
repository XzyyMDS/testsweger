{
  "openapi": "3.0.0",
  "info": {
    "title": "XzyyMDS | Rest API",
    "description": "Free Rest API For Everyone.",
    "version": "1.0.3"
  },
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
                paths: {
                    // --- Uploader Routes ---
                    "/uploader": {
                        post: {
                            tags: ["Uploader"],
                            summary: "Upload File",
                            description: "Upload file gambar, video, audio, atau PDF (Maksimal 20MB).",
                            requestBody: {
                                content: {
                                    "multipart/form-data": {
                                        schema: {
                                            type: "object",
                                            properties: {
                                                file: {
                                                    type: "string",
                                                    format: "binary"
                                                }
                                            }
                                        }
                                    }
                                }
                            },
                            responses: {
                                200: { description: "Berhasil upload file" }
                            }
                        }
                    },
                }
            };